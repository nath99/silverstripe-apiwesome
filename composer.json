{
	"name": "nglasl/silverstripe-apiwesome",
	"description": "A module for SilverStripe which will automatically create customisable JSON/XML feeds for your data objects (including pages), and provides a modular security token that can be used for other applications.",
	"type": "silverstripe-vendormodule",
	"keywords": [
		"silverstripe",
		"api",
		"feed",
		"json",
		"xml"
	],
	"homepage": "https://github.com/nglasl/silverstripe-apiwesome",
	"license": "BSD-3-Clause",
	"authors": [{
		"name": "Nathan Glasl",
		"email": "nathan@symbiote.com.au"
	}],
	"require": {
		"silverstripe/cms": "^4",
		"silverstripe/framework": "^4",
		"silverstripe/vendor-plugin": "^1.6"
	},
	"extra": {
		"installer-name": "apiwesome",
		"branch-alias": {
			"dev-master": "2.2.x-dev"
		},
		"screenshots": [
			"https://raw.githubusercontent.com/nglasl/silverstripe-apiwesome/master/images/apiwesome-visibility.png"
		]
	},
	"config": {
		"allow-plugins": {
			"composer/installers": true,
			"silverstripe/vendor-plugin": true
		}
	}
}
